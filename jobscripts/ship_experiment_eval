python eval/eval.py -n new_ship_sym_loss_exp_v2 -c conf/exp/srn_sym.conf -D /scratch/s2576597/pixel_nerf_datasets/ship_renders_train_upper_hemisphere_30_fov_v2_pixel_nerf/ships --gpu_id=0 --split=test --eval_view_list=../sncar_viewlist.txt --viewlist=../snship_test_views.txt --output=/scratch/s2576597/pixel_nerf_eval_output/snship/sym_loss --write_compare --free_pose
python eval/eval.py -n new_ship_no_mirror_exp_v2_short -c conf/exp/srn.conf -D /scratch/s2576597/pixel_nerf_datasets/ship_renders_train_upper_hemisphere_30_fov_v2_pixel_nerf/ships --gpu_id=0 --split=test --eval_view_list=../sncar_viewlist.txt --viewlist=../snship_test_views.txt --output=/scratch/s2576597/pixel_nerf_eval_output/snship/no_mirror --write_compare --free_pose
